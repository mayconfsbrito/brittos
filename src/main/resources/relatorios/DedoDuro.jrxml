	<queryString>
		<![CDATA[SELECT
*
FROM
usuario u, transacao t
WHERE
If($P{nomeUsuario} like 'Todos',
	IF($P{periodo} like 'Todos',
		u.idUsuario, 	 
		(t.data>=$P{periodoInicial} AND t.data<=$P{periodoFinal})), 
	u.nome like $P{nomeUsuario})

AND u.idUsuario=t.idUsuario

GROUP BY
u.idUsuario]]>
	</queryString>
